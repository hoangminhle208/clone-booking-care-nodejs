<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css">
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.3/dist/jquery.slim.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js"></script>

</head>
<body>
    <div class="table-responsive">
    <table class="table">
        <tr>
            <th>ID</th>
            <th>firstName</th>
            <th>lastName</th>
            <th>email</th>
            <th>Sex</th>
            <th>Phone</th>
            <th>Action</th>
        </tr>
        <% for(let i=0;i< dataTable.length;i++){%>
        <tr>
            <td><%=dataTable[i].id%></td>
            <td><%=dataTable[i].firstName%></td>
            <td><%=dataTable[i].lastName%></td>
            <td><%=dataTable[i].email%></td>
            <td><%=dataTable[i].gender%></td>
            <td><%=dataTable[i].phonenumber%></td>
            <td>
                <a href="/edit-crud?id=<%=dataTable[i].id%>" class="btn btn-success" type="button">EDIT</a>
                <a href="/delete-crud?id=<%=dataTable[i].id%>" class="btn btn-warning" type="button">DELETE</a>
            </td>
        </tr>
            <%}
        %>
    </table>
    </div>
</body>
</html>